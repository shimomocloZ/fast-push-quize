rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /profile {
      allow read, write: if
          request.time < timestamp.date(2022, 5, 28);
    }
    match /users {
      allow read, write: if request.auth != null;
    }
    match /rooms {
      allow read, write: if request.auth != null;
    }
    match /participants {
      allow read, write: if request.auth != null;
    }
    match /answers {
      allow read, write: if request.auth != null;
    }
  }
}
